<UserControl x:Name="userControl" x:Class="TokikuNew.Controls.DockBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TokikuNew.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="27" d:DesignWidth="130"
             Loaded="userControl_Loaded"
             Background="Transparent" >
    <StackPanel Grid.Row="1" Orientation="Horizontal" >
        <Button Margin="2" Width="22" Name="btnF9" FontSize="15" Content="&#xE10E;" FontFamily="/Resources/#Segoe MDL2 Assets" ToolTip="取消" Click="btnF9_Click" Style="{DynamicResource ReadModeSwitch_Button}" >
        </Button>
        <Button Margin="2" Width="22" x:Name="btnF2" FontSize="15" Content="&#xE104;" FontFamily="/Resources/#Segoe MDL2 Assets" ToolTip="修改" Click="btnF2_Click" Style="{DynamicResource ReadModeSwitch_ModifyButton}"/>
        <Button Style="{DynamicResource ReadModeSwitch_SaveButton}" Margin="2" Width="22" Name="btnF3" FontSize="15" Content="&#xE105;" FontFamily="/Resources/#Segoe MDL2 Assets" ToolTip="儲存" Click="btnF3_Click" >
        </Button>
    </StackPanel>
</UserControl>
