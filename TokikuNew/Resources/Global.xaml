<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Tokiku.ViewModels;assembly=Tokiku.ViewModels"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:ValueConverters="clr-namespace:TokikuNew.ValueConverters">
    <ObjectDataProvider x:Key="MaterialCategoriesSource" ObjectType="{x:Type vm:MaterialCategoriesViewModelCollection}" MethodName="Query"/>
    <ObjectDataProvider x:Key="StatesSource" ObjectType="{x:Type vm:StatesViewModelCollection}" MethodName="Query">
    </ObjectDataProvider>
    <ObjectDataProvider x:Key="TicketTypesSource" ObjectType="{x:Type vm:TicketTypesViewModelCollection}" MethodName="Query"/>
    <ObjectDataProvider x:Key="TicketPeriodsSource" ObjectType="{x:Type vm:TicketPeriodsViewModelCollection}" MethodName="Query"/>
    <ObjectDataProvider x:Key="TranscationCategoriesSource" ObjectType="{x:Type vm:TranscationCategoriesViewModelCollection}" MethodName="Query"/>
    <ObjectDataProvider x:Key="PaymentTypesSource" ObjectType="{x:Type vm:PaymentTypesManageViewModelCollection}" MethodName="Query"/>
    <ObjectDataProvider x:Key="VoidStateListSource" ObjectType="{x:Type vm:VoidViewModelCollection}" MethodName="Query"/>
    <ObjectDataProvider x:Key="ConstructionAtlasReplySelectList" ObjectType="{x:Type vm:ConstructionAtlasReplyContextViewModelCollection}" MethodName="Query"/>
    
    <vm:預付款保證票ViewModelCollection x:Key="預付款保證票清單"/>
    <vm:保固票ViewModelCollection x:Key="保固票清單"/>
    <vm:PromissoryNoteManagementViewModelCollection x:Key="PromissoryNoteManagementSource"/>
    <ValueConverters:DateTime2StringFormatingValueConverter x:Key="FormatedDatetime"/>
    <ValueConverters:ReplyContentValueConverter x:Key="AtlasReplayContents"/>
    <ValueConverters:NOTGateValueConverter x:Key="NOTGateValueConverter"/>
    <ValueConverters:RadioButtonSwitchValueConverter x:Key="RadioButtonSwitchValueConverter" />
    <ValueConverters:BooleanToVisibilityValueConveter x:Key="BooleanToVisibilityValueConveter"/>
    <ValueConverters:DocumentLifeCycleStateToVisibilityValueConverter x:Key="DocumentLifeCycleStateToVisibilityValueConverter"/>
    <ValueConverters:ReadModeToDisabledValueConverter x:Key="ReadModeToDisabledValueConverter"/>
    <ValueConverters:StatesCodeToTextValueConverter x:Key="StatesCodeToTextValueConverter"/>
    <ValueConverters:ReadModeToVisibilityValueConverter x:Key="ReadModeToVisibilityValueConverter"/>
    <ValueConverters:ReadModeToEnabledValueConverter x:Key="ReadModeToEnabledValueConverter" />
    <ValueConverters:Number2DateTimeValueConverter x:Key="Number2DateTimeValueConverter"/>
    <ValueConverters:VoidToStateTextValueConverter x:Key="Void2TextValueConvterver" />
    <BitmapImage x:Key="Logo" UriSource="/Resources/16x16.png" />
    <BitmapImage x:Key="Banner" UriSource="/Resources/TokikuLogo.gif" />
    <RoutedUICommand x:Key="Modify" Text="修改" />
    <RoutedUICommand x:Key="Save" Text="暫存"/>
    <RoutedUICommand x:Key="Query" Text="查詢"  />
    <RoutedUICommand x:Key="Commit" Text="確認送出"/>
    <RoutedUICommand x:Key="Features" Text="功能"/>
    <RoutedUICommand x:Key="RunExcel" Text="Run Excel App"/>
    <RoutedUICommand x:Key="Cancel" Text="取消" />
    <RoutedUICommand x:Key="AddColumn" Text="新增行"/>
    <RoutedUICommand x:Key="AddRow" Text="新增列"/>
    <RoutedUICommand x:Key="OpenNewTabItem" Text="開新分頁" />
    <RoutedUICommand x:Key="CloseTabItem" Text="關閉分頁"/>
    <RoutedUICommand x:Key="ResetFiliter" Text="重設查詢" />
    <RoutedUICommand x:Key="QueryRefresh" Text="重新整理查詢" />
    <RoutedUICommand x:Key="Reply" Text="轉送" />
    <Style TargetType="TextBlock">
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>
    <Style TargetType="DataGridCell">
        <Setter Property="FontSize" Value="14"/>
    </Style>
</ResourceDictionary>